#include <iostream>
using namespace std;

int main() {
int t;
cin>>t;
for(int i=0;i<t;i++){
    int x,y;
    cin>>x>>y;
    if((2*x)>(5*y)){
        cout<<"chocolate"<<endl;
    }
    else if((2*x)<(5*y)){
        cout<<"candy"<<endl;
    }
    else{
        cout<<"either"<<endl;
    }
}
	return 0;
}
